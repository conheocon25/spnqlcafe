Options +FollowSymlinks
RewriteEngine on

#---------------------------------------------------------------------------------------------------------------------------
# SYSTEM
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^signin/load$ 			?cmd=SigninLoad [L]
RewriteRule ^signin/exe$ 			?cmd=SigninExe [L]
RewriteRule ^signout/load$ 			?cmd=SignoutLoad [L]
RewriteRule ^signout/exe$ 			?cmd=SignoutExe [L]
RewriteRule ^change/pass/load$ 		?cmd=ChangePassLoad [L]
RewriteRule ^change/pass/exe$ 		?cmd=ChangePassExe [L]

RewriteRule ^home$ 					?cmd=Home [L]
RewriteRule ^contact/ins/exe$ 		?cmd=ContactInsExe [L]

RewriteRule ^object/load/([A-Za-z0-9]+)/([0-9]+)$ 			?cmd=ObjectLoad&ObjectName=$1&Id=$2 [L]
RewriteRule ^object/del/([A-Za-z0-9]+)/([0-9]+)$ 			?cmd=ObjectDel&ObjectName=$1&Id=$2 [L]
RewriteRule ^object/ins/([A-Za-z0-9]+)$ 					?cmd=ObjectIns&ObjectName=$1 [L]
RewriteRule ^object/upd/([A-Za-z0-9]+)$ 					?cmd=ObjectUpd&ObjectName=$1 [L]

RewriteRule ^image/uploading$ 								?cmd=ImageUploading [L]
RewriteRule ^image/deleting/([0-9]+)$ 						?cmd=ImageDeleting&IdImage=$1 [L]


#---------------------------------------------------------------------------------------------------------------------------
# SYSTEM.BLOG
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^([a-z0-9_-]+)$ 								?cmd=Blog&IdKey=$1 [L]
RewriteRule ^([a-z0-9_-]+)/album/([0-9]+)$ 					?cmd=BlogAlbum&IdKey=$1&IdAlbum=$2 [L]
RewriteRule ^([a-z0-9_-]+)/signin/load$ 					?cmd=BlogSigninLoad&IdKey=$1 [L]
RewriteRule ^([a-z0-9_-]+)/signin/exe$ 						?cmd=BlogSigninExe&IdKey=$1 [L]

#---------------------------------------------------------------------------------------------------------------------------
# BLOG.SETTING
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^([a-z0-9_-]+)/setting$ 						?cmd=Setting&IdKey=$1 [L]
RewriteRule ^([a-z0-9_-]+)/setting/info$ 					?cmd=SettingInfo&IdKey=$1 [L]
RewriteRule ^([a-z0-9_-]+)/setting/tag$ 					?cmd=SettingBTag&IdKey=$1 [L]
RewriteRule ^([a-z0-9_-]+)/setting/news$ 					?cmd=SettingBNews&IdKey=$1 [L]
RewriteRule ^([a-z0-9_-]+)/setting/music$ 					?cmd=SettingBMusic&IdKey=$1 [L]
RewriteRule ^([a-z0-9_-]+)/setting/album$ 					?cmd=SettingBAlbum&IdKey=$1 [L]
RewriteRule ^([a-z0-9_-]+)/setting/album/([0-9])$ 			?cmd=SettingBImage&IdKey=$1&IdAlbum=$2 [L]